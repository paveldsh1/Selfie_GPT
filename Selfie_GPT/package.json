{
  "name": "selfie-gpt-bot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:debug": "cross-env NODE_OPTIONS=\"--inspect\" next dev",
    "dev:debug:win": "powershell -Command \"$env:NODE_OPTIONS='--inspect'; next dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "prisma:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "worker": "tsx jobs/image-generate.ts",
    "worker:debug": "tsx --inspect jobs/image-generate.ts",
    "face-worker": "tsx jobs/face-detect.ts",
    "face-worker:debug": "tsx --inspect jobs/face-detect.ts",
    "cleanup": "tsx jobs/cleanup.ts",
    "cleanup:debug": "tsx --inspect jobs/cleanup.ts",
    "prepare": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@tensorflow-models/blazeface": "^0.1.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "@vladmandic/face-api": "^1.7.12",
    "axios": "^1.7.7",
    "bullmq": "^5.12.1",
    "dotenv": "^16.4.5",
    "form-data": "^4.0.0",
    "ioredis": "^5.4.1",
    "jpeg-js": "^0.4.4",
    "mime-types": "^2.1.35",
    "next": "^14.2.5",
    "node-cron": "^3.0.3",
    "openai": "^4.56.0",
    "pino": "^9.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "prettier": "^3.3.3",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.4",
    "typescript": "^5.5.4"
  }
}
